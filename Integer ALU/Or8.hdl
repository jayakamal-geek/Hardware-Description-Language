// x OR y, where x and y have 8 bits (bitwise)
CHIP Or8 {
	IN x[8], y[8];
	OUT z[8];
	
	PARTS:
	Or(a=x[0], b=y[0], out=z[0]);
	Or(a=x[1], b=y[1], out=z[1]);
	Or(a=x[2], b=y[2], out=z[2]);
	Or(a=x[3], b=y[3], out=z[3]);
	Or(a=x[4], b=y[4], out=z[4]);
	Or(a=x[5], b=y[5], out=z[5]);
	Or(a=x[6], b=y[6], out=z[6]);
	Or(a=x[7], b=y[7], out=z[7]);
}
